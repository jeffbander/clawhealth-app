{
  "crons": [
    {
      "path": "/api/cron/daily-alerts",
      "schedule": "0 12 * * *"
    },
    {
      "path": "/api/cron/proactive-outreach",
      "schedule": "0 14 * * *"
    },
    {
      "path": "/api/cron/memory-consolidation",
      "schedule": "0 2 * * *"
    }
  ],
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin"
        }
      ]
    }
  ]
}
